@model ClickServ2022.Models.Login

@{
    ViewData["Title"] = "Index";
}
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
</head>
<body id="corpo-login">

    <img src="~/imagens/logo-novo.png" asp-append-version="true" id="logo-login">


    <section>
        <input name="usuario" id="usuario-input" class="form-control" placeholder="USUÁRIO">
        <input type="password" name="Senha" id="senha-input" class="form-control" placeholder="SENHA">
        <input type="submit" value="ENTRAR" id="botao-login" class="btn btn-primary" />
    </section>

</body>

</html>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}